# SendGrid Email Provider Configuration
# Copy this file to .env and fill in your values

# Default email provider
MESSAGE_DEFAULT_EMAIL_PROVIDER=sendgrid

# SendGrid API credentials
MESSAGE_SENDGRID_API_KEY=your-sendgrid-api-key

# Sender information
MESSAGE_SENDGRID_FROM_EMAIL=noreply@yourdomain.com
MESSAGE_SENDGRID_FROM_NAME=Your App Name
