# Amazon SES Email Provider Configuration
# Copy this file to .env and fill in your values

# Default email provider
MESSAGE_DEFAULT_EMAIL_PROVIDER=ses

# AWS SES API credentials
MESSAGE_SES_API_KEY=your-aws-access-key-id
MESSAGE_SES_API_SECRET=your-aws-secret-access-key
MESSAGE_SES_REGION=us-east-1

# Sender information
MESSAGE_SES_FROM_EMAIL=noreply@yourdomain.com
MESSAGE_SES_FROM_NAME=Your App Name
