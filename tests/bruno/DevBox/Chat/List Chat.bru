meta {
  name: List Chat
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/api/v1/chat
  body: none
  auth: none
}

tests {
  test("should return 200", function() {
    expect(res.status).to.equal(200);
  });

  test("should return array", function() {
    expect(res.body).to.be.an("array");
  });

  test("should have chat with required fields", function() {
    if (res.body.length > 0) {
      const chat = res.body[0];
      expect(chat).to.have.property("id");
      expect(chat).to.have.property("chat");
      expect(chat).to.have.property("created_at");
    }
  });
}
